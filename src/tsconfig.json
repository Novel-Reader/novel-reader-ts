  "compilerOptions": {
    "jsx": "react",
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node10",
    "resolveJsonModule": true,
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
